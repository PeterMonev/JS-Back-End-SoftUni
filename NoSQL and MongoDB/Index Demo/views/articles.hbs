<h1>Articles</h1>
<form action="/articles" method="post">
   <label>Author <input type="text" name="author"></label>
    <label>Tilte <input type="text" name="tilte"></label>
     <label>Content<textarea name="content" id="" cols="30" rows="10"></textarea></label>
</form>
<ul>
    {{#each articles}}
    <li>
        <header>{{title}}</header>
        <main>{{content}}</main>
        <footer>{{author}}</footer>
    </li>
    {{/each }}
</ul>