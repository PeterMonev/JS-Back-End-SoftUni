<section>
<h1>All Accomodations</h1>

 <div>
    <form method="get">
        <input name="search" value="{{search}}">
        <input type="submit" value="Search">
    </form>
 </div>


{{#if rooms}}

    <ul>
        {{#each rooms}}
        {{> roomCard}}
        {{/each}}
    </ul>
    {{else}}

    <p>Nothing has been listed yet.</p>

  {{/if}}
</section>